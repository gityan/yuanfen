<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	 
    <!--state_pressed如果是true，当被点击时显示该图片，如果是false没被按下时显示默认。-->
    <!--state_focused true，获得焦点时显示；false，没获得焦点显示默认。 -->
    <!--state_selected  true，当被选择时显示该图片；false，当未被选择时显示该图片。-->
	 <item 
		android:state_pressed="true" 
		android:color="#ffffffff" />
	 <item 
		android:state_pressed="false" 
		android:state_focused="false"
		android:color="#ff000000" />
	 <!--  
	 <item 
		android:state_focused="true" 
		android:drawable="@drawable/temp3" />
	 <item 
		android:state_focused="false" 
		android:drawable="@drawable/temp4" />
		-->
</selector>